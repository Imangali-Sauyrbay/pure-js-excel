{
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "google"],
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "object-curly-spacing":"off",
        "operator-linebreak": "off",
        "require-jsdoc":"off",
        "linebreak-style":"off"
    }
}